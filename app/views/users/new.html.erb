<%= form_for(@user) do |u| %>
    <%= u.label :Login %> 
    <%= u.text_field :Login %></br>

    <%= u.label :Password %> 
    <%= u.text_field :Password %></br>

    <%= u.label :Email %> 
    <%= u.text_field :Email %></br>

    <%= u.label :FirstName %> 
    <%= u.text_field :FirstName %></br>

    <%= u.label :LastName %> 
    <%= u.text_field :LastName %></br>

    <%= u.label :Age %> 
    <%= u.text_field :Age %></br>

    <%= u.label :Sex %> 
    <%= u.text_field :Sex %></br>

    <%= u.label :Facebook %> 
    <%= u.text_field :Facebook %></br>

    <%= u.label :IsPrivate %> 
    <%= u.text_field :IsPrivate %></br>

    <%= u.submit "Create"  %></br>

<% end %>

    

    

    